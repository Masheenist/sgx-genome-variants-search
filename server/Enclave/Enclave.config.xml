<EnclaveConfiguration>
	<ProdID>0</ProdID>
	<ISVSVN>0x1</ISVSVN>

	<!-- If TCSMaxNum == TCSNum == TCSMinPool, there is no dynamic thread -->
	<!-- If TCSMaxNum is omitted, it defaults to TCSNum -->
	<!-- This means currently 8 static threads are available for this configuration -->
	<TCSMaxNum>1</TCSMaxNum>
	<TCSNum>1</TCSNum>
	<TCSMinPool>1</TCSMinPool>
	<TCSPolicy>1</TCSPolicy>

	<StackMaxSize>0x040000</StackMaxSize>
	<HeapMaxSize>0x8000000</HeapMaxSize>

	<!-- Recommend changing 'DisableDebug' to 1 to make the enclave undebuggable for enclave release -->
	<DisableDebug>0</DisableDebug>
	<MiscSelect>0</MiscSelect>
	<MiscMask>0xFFFFFFFF</MiscMask>
</EnclaveConfiguration>
